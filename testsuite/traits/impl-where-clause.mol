
//! true
 
import lib.libcore

trait TraitTest {
    decl thing(Self) -> Bool
}

impl TraitTest for String {
    fn thing(self) {
        println(self)
        true
    }
}


impl TraitTest for List<'p> where p: TraitTest {
    fn thing(self) {
        true
    }
}

fn test_trait(t: 'a) where a: TraitTest {
    println(str(t.thing()))
}

test_trait(new List<String>())



